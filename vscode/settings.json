// Place your settings in this file to overwrite the default settings
{
    "terminal.integrated.shell.osx": "/bin/zsh",
    "editor.renderControlCharacters": true,
    "workbench.iconTheme": "vscode-icons",
    "window.zoomLevel": 0,
    "editor.fontSize": 14,
    "editor.minimap.enabled": true,
    "workbench.colorTheme": "Monokai++",
    "extensions.ignoreRecommendations": true,
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "files.insertFinalNewline": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "files.trimTrailingWhitespace": true,
    "diffEditor.renderSideBySide": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true
    },
    "breadcrumbs.enabled": true,
    "cSpell.language": "en,fr",
    "editor.tabCompletion": "on",
    "gitlens.views.repositories.files.layout": "tree",
    "search.showLineNumbers": true,
    "workbench.editor.highlightModifiedTabs": true,
    "editor.cursorSmoothCaretAnimation": false,
    "cSpell.ignorePaths": [
        "yarn.lock"
    ],
    "editor.tabSize": 2,
    "workbench.sideBar.location": "left",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "terminal.external.osxExec": "iTerm.app",
    "explorer.autoReveal": false,
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "todo-tree.filtering.excludeGlobs": [
        "/Users/ixi/Library/Caches",
        "node_modules"
    ],
    "todo-tree.tree.showScanModeButton": false,
    "debug.showBreakpointsInOverviewRuler": true,
    "eslint.alwaysShowStatus": true,
    "javascript.preferences.useAliasesForRenames": false,
    "typescript.preferences.useAliasesForRenames": false,
    "editor.accessibilitySupport": "off",
    "editor.accessibilityPageSize": 1,
    "explorer.compactFolders": false,
    "askToolkit.showWelcomeScreen": false,
    "ask.telemetryEnabled": "Disabled",
    "rest-client.enableTelemetry": false,
    "code-runner.enableAppInsights": false,
    "errorLens.exclude": [
        "Unknown word"
    ],
    "explorer.openEditors.sortOrder": "alphabetical",
    "editor.minimap.size": "fit",
    "workbench.editor.decorations.colors": true,
    "workbench.editor.decorations.badges": true,
    "emmet.showAbbreviationSuggestions": false,
    "emmet.showExpandedAbbreviation": "never",
    "terminal.integrated.tabs.enabled": true,
    "typescript.autoClosingTags": false,
    "javascript.autoClosingTags": false,
    "html.autoClosingTags": false,
    "workbench.startupEditor": "none",
    "editor.gotoLocation.multipleReferences": "goto",
    "cSpell.diagnosticLevel": "Hint",
    "workbench.editor.sharedViewState": true,
    "git.openDiffOnClick": false,
    "editor.bracketPairColorization.enabled": true,
    "typescript.inlayHints.parameterNames.enabled": "literals",
    "javascript.inlayHints.parameterNames.enabled": "literals",
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "editor.suggest.preview": true,
    "editor.guides.bracketPairs": "active",
    "editor.renderLineHighlight": "all",
    "references.preferredLocation": "view",
    "scm.diffDecorationsIgnoreTrimWhitespace": "inherit",
    "editor.unicodeHighlight.nonBasicASCII": true,
    "editor.unicodeHighlight.includeComments": true,
    "editor.suggest.showSnippets": false,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": false,
        "markdown": true
    },
    "editor.guides.indentation": true,
    "typescript.suggest.completeFunctionCalls": true,
    "scm.diffDecorationsGutterPattern": {
        "added": true
    },
    "debug.autoExpandLazyVariables": true,
    "jest.showTerminalOnLaunch": false
}